{
  "name": "mml-syntax-hightlight",
  "displayName": "mml-syntax-hightlight",
  "description": "basic syntax hightlight for MML (man machine language)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mml",
      "aliases": ["mml", "mml"],
      "extensions": [".mml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mml",
      "scopeName": "source.mml",
      "path": "./syntaxes/mml.tmLanguage.json"
    }]
  }
}
